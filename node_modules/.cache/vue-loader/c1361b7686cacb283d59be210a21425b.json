/lib/component/dataZoom\";\n\nexport default {\n  name: \"clChartPie\",\n  mounted() {\n    this.selfAdaption();\n  },\n  data() {\n    return {\n      option:{\n          title: {\n    text: '玫瑰图',\n    x: 'center'\n  },\n  tooltip: {\n    trigger: 'item',\n    formatter: '{a} <br/>{b} : {c} ({d}%)'\n  },\n  legend: {\n    orient: 'vertical',\n    left: 'left',\n    data: ['rose1','rose2','rose3','rose4','rose5','rose6','rose7','rose8']\n  },\n  series: [\n    {\n      name: '玫瑰图',\n      type: 'pie',\n      roseType : 'radius',\n      radius: '55%',\n      center: ['50%', '60%'],\n      data: [\n                {value:10, name:'rose1'},\n                {value:5, name:'rose2'},\n                {value:15, name:'rose3'},\n                {value:25, name:'rose4'},\n                {value:20, name:'rose5'},\n                {value:35, name:'rose6'},\n                {value:30, name:'rose7'},\n                {value:40, name:'rose8'}\n      ],\n      itemStyle: {\n        emphasis: {\n          shadowBlur: 10,\n          shadowOffsetX: 0,\n          shadowColor: 'rgba(0, 0, 0, 0.5)'\n        }\n      }\n    }\n  ]\n      }\n    };\n  },\n  props: {\n  },\n  watch: {\n  },\n  components: { chart: ECharts },\n  methods: {\n    selfAdaption() {\n      let _this = this;\n      setTimeout(() => {\n        window.addEventListener(\"resize\", function() {\n          if (_this.$refs.chartsAll) {\n            _this.$refs.chartsAll.resize();\n          }\n        });\n      }, 10);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.chartsAll {\n  height: 100%;\n  width: 100%;\n}\n</style>\n"]}]}