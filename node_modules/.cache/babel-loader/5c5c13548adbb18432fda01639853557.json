    window.addEventListener(\"resize\", function() {\n          if (_this.$refs.chartsAll) {\n            _this.$refs.chartsAll.resize();\n          }\n        });\n      }, 10);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.chartsAll {\n  height: 100%;\n  width: 100%;\n}\n</style>\n"],"sourceRoot":"src/components/echarts"}]}